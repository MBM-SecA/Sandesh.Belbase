@model Person
@{
    ViewData["Titlephrase"]=@Model.FirstName;
}
<h1>Detail For Employee  <a class="btn btn-outline-primary" href="/employee/index" role="button">Go Back</a></h1>

<ul class="list-group">
<li class="list-group-item">First Name: @Model.FirstName</li>
<li class="list-group-item">Last Name: @Model.LastName</li>
<li class="list-group-item">Address: @Model.Address</li>
<li class="list-group-item">Gender: @Model.Gender</li>
<li class="list-group-item">Saalary: @Model.Salary</li>
</ul>
